<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AM_NonfunctionalBuilding" Name ="AM_NonfunctionalBuilding_Package" Abstract="True">
        <minifiedDef>MinifiedThing</minifiedDef>
        <thingCategories>
			<li>AM_Container</li>
	  </thingCategories>
        <building>
          <deconstructible>true</deconstructible>    
          <claimable>true</claimable>
			<isEdifice>false</isEdifice>
        </building>
		 <altitudeLayer>BuildingOnTop</altitudeLayer>
       <clearBuildingArea>false</clearBuildingArea>
       <canOverlapZones>true</canOverlapZones>
        <pathCost>20</pathCost>
        <fillPercent>0.20</fillPercent>
        <statBases>
        <MaxHitPoints>150</MaxHitPoints>
          <Mass>35</Mass>
          <MarketValue>10</MarketValue>
        </statBases>
		<designationCategory>AM_FLOOR</designationCategory>
    </ThingDef>

    <ThingDef ParentName="AM_NonfunctionalBuilding_Package">
        <defName>AM_LargeBackpack_Decoration</defName>
        <label>large backpack</label>
        <description>an oversized military backpack that is already broken.</description>
        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <texPath>Things/Bags/LargeBackpackB</texPath>
            <drawSize>1.5</drawSize>
        </graphicData>
        <size>(1,1)</size>
    </ThingDef>

