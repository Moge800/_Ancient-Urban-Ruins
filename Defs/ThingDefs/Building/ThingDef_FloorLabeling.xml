	<ThingDef ParentName="BuildingBase">
		<defName>AM_IndicatorLine_Yellow</defName>
		<label>Indicator line</label>
		<description>.</description>
    	<thingClass>Building</thingClass>
    	<category>Building</category>
    	<graphicData>
      		<texPath>Things/Building/FloorLabeling/IndicatorLine_Yellow</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			 <linkType>Basic</linkType>
            <linkFlags>
              <li>Custom10</li>
            </linkFlags>
    	</graphicData>
		 <uiIconPath>Things/Building/FloorLabeling/IndicatorLine_Yellow</uiIconPath> 
    	<altitudeLayer>SmallWire</altitudeLayer>
    	<passability>Standable</passability>
        <useHitPoints>false</useHitPoints>
    	<placingDraggableDimensions>1</placingDraggableDimensions>
    	<costList>
    	  <Steel>8</Steel>
    	</costList>
    	<rotatable>false</rotatable>
    	<selectable>false</selectable>
    	<clearBuildingArea>false</clearBuildingArea>
		<building>
			<isEdifice>false</isEdifice>
		</building>
				<designationCategory>AM_FLOOR</designationCategory>
  	</ThingDef>
	
