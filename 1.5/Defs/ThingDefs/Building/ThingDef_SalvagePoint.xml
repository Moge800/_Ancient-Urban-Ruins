<Defs>

    <ThingDef ParentName="PortalBase" Name="AM_PortalBase_SalvagePoint" Abstract="True">
        <category>Building</category>
        <thingClass>AncientMarket_Libraray.Lootbox</thingClass>
        <rotatable>true</rotatable>
		<useHitPoints>true</useHitPoints>
		<destroyable>true</destroyable>
        <building>
            <expandHomeArea>false</expandHomeArea>
            <claimable>true</claimable>
			<deconstructible>true</deconstructible> 
        </building>
		<statBases>
       <MaxHitPoints>200</MaxHitPoints>
      </statBases>
		<pathCost>40</pathCost>
      <fillPercent>0.40</fillPercent>
    </ThingDef>
	
	<ThingDef ParentName="AM_PortalBase_SalvagePoint" Name="AM_PortalBase_SalvagePoint_Unloadable" Abstract="True">
      <minifiedDef>MinifiedThing</minifiedDef>
	  <thingCategories>
			<li>AM_Container</li>
	  </thingCategories>
      <building>
        <deconstructible>true</deconstructible>    
        <claimable>true</claimable>
      </building>
      <pathCost>20</pathCost>
      <fillPercent>0.20</fillPercent>
      <statBases>
	  <MaxHitPoints>150</MaxHitPoints>
        <Mass>35</Mass>
        <MarketValue>10</MarketValue>
      </statBases>
	  <tradeTags>
      <li>AM_SalvagePoint</li>
    </tradeTags>
    </ThingDef>
	
	
